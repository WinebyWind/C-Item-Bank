# include <stdio.h>
int  main()
{ 
  long int i,a1,a2,a3,a4,a5,j=0;
  for(i=100;i<100000;i++){
     a1=i/10000;
     a2=(i-a1*10000)/1000;
     a3=(i-a1*10000-a2*1000)/100;
     a4=(i-a1*10000-a2*1000-a3*100)/10;
     a5=i-a1*10000-a2*1000-a3*100-a4*10;
     if(a1+a2+a3+a4+a5==5){
        printf("%ld  ",i);
        j=j+1; 
     } 
   }
   printf("整数100至100000之间所有数字之和为5的整数的总个数为：\"%ld\"\n",j);
   return 0;
}
